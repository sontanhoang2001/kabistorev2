RewriteEngine On
#RewriteCond %{REQUEST_FILENAME} !-f

#RewriteRule ^([^\.]+)$ $1.php [NC,L]
#RewriteRule ^(.*)\.php$ $1.html [NC,L]
#RewriteRule ^(.*)\.html$  $1.php [NC,L]


#https://youtu.be/Qavbzr22OKs
Options All -Indexes

RewriteCond %{REQUEST_FILENAME} !-d
RewriteCond %{REQUEST_FILENAME}\.php  -f
RewriteRule ^details/([0-9]*)/([a-zA-Z0-9_-]+)\.html$ details.php?proid=$1&key=$2
RewriteRule ^san-pham/([a-zA-Z0-9_-]+)-([0-9]*)-([0-9]*)s([a-zA-Z0-9_-]+)e([a-zA-Z0-9_-]+)\.html$ products.php?filter=$1&page=$2&type=$3&priceStart=$4&priceEnd=$5

#RewriteRule ^san-pham/([a-zA-Z0-9_-]+)/?p([0-9]*)f([0-9]*)s([a-zA-Z0-9_-]*)e([a-zA-Z0-9_-]*)\.html$ products.php?type=$1&page=$2&filter=$3&priceStart=$4&priceEnd=$5

RewriteRule ^orderdetails-([0-9]*)\.html$ orderdetails.php?page=$1
RewriteRule ^wishlist-([0-9]*)\.html$ wishlist.php?page=$1





RewriteRule ^(.*)\.html$  $1.php [NC,L]
